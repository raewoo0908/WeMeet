{
  "rule_id": "f7d7ebd5-a016-46e2-9c54-f9932f2d386d",
  "risk_score": 73,
  "description": "Execution of ssh.exe to perform data exfiltration and tunneling through RDP",
  "name": "Potential RDP Tunneling Via SSH",
  "severity": "high",
  "type": "query",
  "language": "lucene",
  "query": "process.command_line:*\\:3389* AND process.executable:*\\\\ssh.exe",
  "filters": [
    {
      "query": {
        "match": {
          "event.action": {
            "type": "phrase",
            "query": "Process Create (rule: ProcessCreate)"
          }
        }
      }
    }
  ],
  "enabled": true,
  "interval": "5m",
  "from": "now-70m",
  "max_signals": 100,
  "tags": [
    "attack.command-and-control",
    "attack.t1572"
  ],
  "to": "now",
