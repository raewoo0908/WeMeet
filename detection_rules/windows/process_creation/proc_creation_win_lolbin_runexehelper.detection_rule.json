{
  "rule_id": "cd71385d-fd9b-4691-9b98-2b1f7e508714",
  "risk_score": 47,
  "description": "Detect usage of the \"runexehelper.exe\" binary as a proxy to launch other programs",
  "name": "Lolbin Runexehelper Use As Proxy",
  "severity": "medium",
  "type": "query",
  "language": "lucene",
  "query": "process.parent.executable:*\\\\runexehelper.exe",
  "filters": [
    {
      "query": {
        "match": {
          "event.action": {
            "type": "phrase",
            "query": "Process Create (rule: ProcessCreate)"
          }
        }
      }
    }
  ],
  "enabled": true,
  "interval": "5m",
  "from": "now-70m",
  "max_signals": 100,
