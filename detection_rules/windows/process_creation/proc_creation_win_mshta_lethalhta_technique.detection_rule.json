{
  "rule_id": "ed5d72a6-f8f4-479d-ba79-02f6a80d7471",
  "risk_score": 73,
  "description": "Detects potential LethalHTA technique where the \"mshta.exe\" is spawned by an \"svchost.exe\" process",
  "name": "Potential LethalHTA Technique Execution",
  "severity": "high",
  "type": "query",
  "language": "lucene",
  "query": "process.executable:*\\\\mshta.exe AND process.parent.executable:*\\\\svchost.exe",
  "filters": [
    {
      "query": {
        "match": {
          "event.action": {
            "type": "phrase",
            "query": "Process Create (rule: ProcessCreate)"
          }
        }
      }
    }
  ],
  "enabled": true,
  "interval": "5m",
  "from": "now-70m",
  "max_signals": 100,
  "tags": [
    "attack.defense-evasion",
    "attack.t1218.005"
  ],
  "to": "now",
