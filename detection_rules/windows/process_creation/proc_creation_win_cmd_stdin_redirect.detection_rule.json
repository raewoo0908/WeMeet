{
  "rule_id": "241e802a-b65e-484f-88cd-c2dc10f9206d",
  "risk_score": 47,
  "description": "Detect the use of \"<\" to read and potentially execute a file via cmd.exe",
  "name": "Read Contents From Stdin Via Cmd.EXE",
  "severity": "medium",
  "type": "query",
  "language": "lucene",
  "query": "process.command_line:*\\<* AND (process.pe.original_file_name:Cmd.Exe OR process.executable:*\\\\cmd.exe)",
  "filters": [
    {
      "query": {
        "match": {
          "event.action": {
            "type": "phrase",
            "query": "Process Create (rule: ProcessCreate)"
          }
        }
      }
    }
  ],
  "enabled": true,
  "interval": "5m",
  "from": "now-70m",
  "max_signals": 100,
  "tags": [
    "attack.execution",
    "attack.t1059.003"
  ],
  "to": "now",
