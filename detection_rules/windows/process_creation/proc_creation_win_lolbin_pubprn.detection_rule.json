{
  "rule_id": "1fb76ab8-fa60-4b01-bddd-71e89bf555da",
  "risk_score": 47,
  "description": "Detects the use of the 'Pubprn.vbs' Microsoft signed script to execute commands.",
  "name": "Pubprn.vbs Proxy Execution",
  "severity": "medium",
  "type": "query",
  "language": "lucene",
  "query": "process.command_line:*\\\\pubprn.vbs* AND process.command_line:*script\\:*",
  "filters": [
    {
      "query": {
        "match": {
          "event.action": {
            "type": "phrase",
            "query": "Process Create (rule: ProcessCreate)"
          }
        }
      }
    }
  ],
  "enabled": true,
  "interval": "5m",
  "from": "now-70m",
  "max_signals": 100,
  "tags": [
    "attack.defense-evasion",
    "attack.t1216.001"
  ],
  "to": "now",
