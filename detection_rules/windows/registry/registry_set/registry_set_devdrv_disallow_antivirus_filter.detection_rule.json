{
  "rule_id": "31e124fb-5dc4-42a0-83b3-44a69c77b271",
  "risk_score": 73,
  "description": "Detects activity that indicates a user disabling the ability for Antivirus mini filter to inspect a \"Dev Drive\".\n",
  "name": "Antivirus Filter Driver Disallowed On Dev Drive - Registry",
  "severity": "high",
  "type": "query",
  "language": "lucene",
